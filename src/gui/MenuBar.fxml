<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MenuBarController">
   <children>
      <TitledPane fx:id="myMainMenu" animated="false" prefHeight="145.0" prefWidth="690.0" text="Main Menu">
         <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="88.0" prefWidth="601.0">
               <children>
                  <Button fx:id="myViewAuctionsButton" mnemonicParsing="false" prefHeight="115.0" prefWidth="230.0" text="View auction" textFill="#f28109">
                     <font>
                        <Font name="System Bold Italic" size="25.0" />
                     </font>
                  </Button>
                  <Button layoutX="230.0" mnemonicParsing="false" prefHeight="111.0" prefWidth="230.0" text="View my Items">
                     <font>
                        <Font name="System Bold Italic" size="23.0" />
                     </font>
                     <textFill>
                        <LinearGradient endX="1.0" endY="1.0" startX="0.6619047619047619">
                           <stops>
                              <Stop color="#09e8e8" />
                              <Stop color="#0fdceb" offset="1.0" />
                           </stops>
                        </LinearGradient>
                     </textFill>
                  </Button>
                  <Button layoutX="460.0" minWidth="100.0" mnemonicParsing="false" prefHeight="115.0" prefWidth="230.0" text="View my bids" textFill="#da1111">
                     <font>
                        <Font name="System Bold Italic" size="25.0" />
                     </font>
                  </Button>
               </children>
            </AnchorPane>
         </content>
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </TitledPane>
   </children>
</VBox>

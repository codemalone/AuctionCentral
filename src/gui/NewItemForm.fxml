<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane stylesheets="@form.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.NewItemFormController">
   <children>
      <Pane id="main" layoutX="20.0" prefHeight="575.0" prefWidth="625.0">
         <children>
            <Label layoutX="14.0" layoutY="6.0" text="Add Item to this Auction:">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <TextField fx:id="myItemDescription" layoutX="154.0" layoutY="91.0" promptText="Item Name" />
            <TextField fx:id="myMinimumBid" layoutX="154.0" layoutY="184.0" promptText="Minimum bid" />
            <Button fx:id="mySubmitButton" layoutX="232.0" layoutY="300.0" mnemonicParsing="false" text="Submit" />
            <Label fx:id="myErrorLabel" layoutX="244.0" layoutY="248.0" text="Label" textFill="#eb1010" />
         </children>
      </Pane>
   </children>
</AnchorPane>
